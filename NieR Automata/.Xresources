! Xft settings
Xft.autohint: 0
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! colors

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #a54242
*.color9:       #cc6666

! green
*.color2:       #8c9440
*.color10:      #b5bd68

! yellow
*.color3:       #de935f
*.color11:      #f0c674

! blue
*.color4:       #5f819d
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #707880
*.color15:      #c5c8c6

urxvt.depth: 32
urxvt.internalBorder: 32

urxvt.cursorUnderline: true
urxvt.cursorBlink: true

urxvt.font: xft:Source Code Pro Semibold:size=10:antialias=true
urxvt.boldFont: xft:Source Code Pro Semibold:bold:size=10:antialias=true

urxvt.scrollBar: false
urxvt.foreground: #48453c
urxvt.background: #d4ceb5
